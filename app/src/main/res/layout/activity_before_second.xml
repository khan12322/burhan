<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".before_second"
    android:background="@color/black">
    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/red"
        android:textAlignment="textEnd"
        app:title="@string/fitness_app_before_18"
        app:titleTextColor="@color/white"
        />
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/step_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/toolbar"
                android:orientation="vertical"
                android:layout_marginTop="10dp"
                android:clickable="true"
                android:focusable="true">


                <LinearLayout
                    android:id="@+id/before_1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:layout_marginTop="15dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:src="@drawable/lunge1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/lunges"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/step_2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_1"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">


                <LinearLayout
                    android:id="@+id/before_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:src="@drawable/pushup"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/pushup"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/step_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_2"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">


                <LinearLayout
                    android:id="@+id/before_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/squats1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/squats"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>



            <LinearLayout
                android:id="@+id/step_4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_3"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">


                <LinearLayout
                    android:id="@+id/before_4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/dumble_row1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/dumbbell_rows"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_4"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">


                <LinearLayout
                    android:id="@+id/before_5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/single_leg1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/Single_leg_deadlifts"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="18dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_5"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">

                <LinearLayout
                    android:id="@+id/before_6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/burpees1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/burpees"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_6"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">

                <LinearLayout
                    android:id="@+id/before_7"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/side_plank1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/side_planks"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_7"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">

                <LinearLayout
                    android:id="@+id/before_8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/planks1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/planks"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_8"
                android:orientation="vertical"
                android:clickable="true"
                android:focusable="true">

                <LinearLayout
                    android:id="@+id/before_9"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/glute_bridge1"/>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:layout_weight="1.0"
                            >
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/Glute_bridge"
                                android:textColor="@color/white"
                                android:fontFamily="@font/roboto_bold"
                                android:textSize="25dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/repeat"
                                android:textColor="@color/black"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:padding="5dp"
                                android:layout_marginTop="5dp"
                                android:layout_marginStart="5dp"
                                android:text="@string/time"
                                android:textColor="@color/white_light"
                                android:fontFamily="@font/roboto_light"
                                android:textSize="20dp"
                                />
                        </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/step_10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/step_9"
                android:orientation="vertical"
                android:layout_marginBottom="70dp"
                android:clickable="true"
                android:focusable="true">

                <LinearLayout
                    android:id="@+id/before_10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical"
                    android:background="@drawable/backgroundround"
                    android:clickable="true"
                    android:focusable="true"
                    >

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="150dp"
                        android:orientation="horizontal"
                        android:layout_gravity="center">
                        <ImageView
                            android:layout_width="150dp"
                            android:layout_height="wrap_content"
                            android:padding="10dp"
                            android:src="@drawable/overhead1"/>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:layout_weight="1.0"
                                >
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="5dp"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginStart="5dp"
                                    android:text="@string/overhead_dumbbell"
                                    android:textColor="@color/white"
                                    android:fontFamily="@font/roboto_bold"
                                    android:textSize="18dp"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="5dp"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginStart="5dp"
                                    android:text="@string/repeat"
                                    android:textColor="@color/black"
                                    android:fontFamily="@font/roboto_light"
                                    android:textSize="20dp"
                                    />
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:padding="5dp"
                                    android:layout_marginTop="5dp"
                                    android:layout_marginStart="5dp"
                                    android:text="@string/time"
                                    android:textColor="@color/white_light"
                                    android:fontFamily="@font/roboto_light"
                                    android:textSize="20dp"
                                    />
                            </LinearLayout>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
